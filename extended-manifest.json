{
  "manifest_version": 2,
  "name": "Another2Tab - New Tab page",
  "short_name": "__MSG_short_name__",
  "description": "__MSG_main_description__",
  "version": "0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "default_locale": "en",
  "offline_enabled": true,
  "storage": {
    "managed_schema": "init.json"
  },
  "incognito": "split",
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage",
    "bookmarks",
    "management",
    "topSites",
    "chrome://favicon/",
    "system.cpu",
    "http://hydralien.net/"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
    ]
  }
}
